CREATE TABLE Geschlecht (PK_ID INTEGER AUTO_INCREMENT NOT NULL, GESCHLECHT VARCHAR(255), PRIMARY KEY (PK_ID))
CREATE TABLE Kanton (PK_ID INTEGER AUTO_INCREMENT NOT NULL, NAME VARCHAR(255), PRIMARY KEY (PK_ID))
CREATE TABLE Patient (PK_ID INTEGER AUTO_INCREMENT NOT NULL, GEBURTSDATUM DATE, GEWICHTKG DOUBLE, fk_geschlecht VARCHAR(255), PRIMARY KEY (PK_ID))
CREATE TABLE R_Patient_Sucht (PK_ID INTEGER AUTO_INCREMENT NOT NULL, SUCHTBEGIN DATE, SUCHTENDE DATE, fk_Patient VARCHAR(255), fk_Status VARCHAR(255), fk_Sucht VARCHAR(255), fk_Suchttyp VARCHAR(255), BEHANDLUNGSBEGIN DATE, BEHANDLUNGSENDE DATE, fk_Spital VARCHAR(255), PRIMARY KEY (PK_ID))
CREATE TABLE Spital (PK_ID INTEGER AUTO_INCREMENT NOT NULL, CW DOUBLE, NAME VARCHAR(255), fk_Kanton VARCHAR(255), PRIMARY KEY (PK_ID))
CREATE TABLE Status (PK_ID INTEGER AUTO_INCREMENT NOT NULL, STATUS VARCHAR(255), PRIMARY KEY (PK_ID))
CREATE TABLE Sucht (PK_ID INTEGER AUTO_INCREMENT NOT NULL, NAME VARCHAR(255), PRIMARY KEY (PK_ID))
CREATE TABLE Suchttyp (PK_ID INTEGER AUTO_INCREMENT NOT NULL, TYP VARCHAR(255), PRIMARY KEY (PK_ID))
